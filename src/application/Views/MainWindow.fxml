<BorderPane fx:controller="application.SecondaryController"
	xmlns:fx="http://javafx.com/fxml" styleClass="root" id="Container">
	<top>
 	<MenuBar id="menuBar">
      <menus>
         <Menu text="File">
            <items>
               <MenuItem text="New"/>
               <MenuItem text="Open"/>
               <MenuItem text="Save"/>
               <MenuItem text="Save As"/>
               <SeparatorMenuItem  />
               <MenuItem text="Exit"/>
            </items>
         </Menu>
         <Menu text="Edit">
            <items>
               <MenuItem text="Edit Sequence" />
               <MenuItem text="Edit Universe Layout" />
            </items>
         </Menu>
         <Menu text="Help">
            <items>
               <MenuItem text="Search" disable="true"/>
               <MenuItem text="Online Manual" visible="false"/>
               <SeparatorMenuItem />
               <MenuItem text="About"/>
            </items>
         </Menu>
      </menus>
  	 </MenuBar>
  	 </top>
