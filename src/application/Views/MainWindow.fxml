	<left>
		<VBox id="LeftPane">
			<Label text="Left side" />
		</VBox>
	</left>
	<center>
		<VBox id="RightPane">
			<Label text="Center side" />
		</VBox>
	</center>
	<bottom>
		<VBox id="BottomPane">
			<Label text="Bottom"></Label>
		</VBox>
	</bottom>
	<stylesheets>
		<URL value="@application.css" />
	</stylesheets>
	
</BorderPane>
