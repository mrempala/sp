<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.* ?>
<?import javafx.scene.control.* ?>

<TabPane fx:controller="application.VcPtVisualSchematicViewClickable" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
layoutX="20.0" layoutY="12.0" prefHeight="306.0" prefWidth="757.0" id="VisSchem" tabClosingPolicy="UNAVAILABLE">
  <tabs>
    <Tab text="Schematic">
      <content>
       <AnchorPane id="VisSchemSchematic">
        <AnchorPane fx:id="schematicContainer" minHeight="0.0" minWidth="0.0" prefHeight="300.0" prefWidth="200.0" >
        <Button layoutX="10" layoutY="240" mnemonicParsing = "false" text = "Origin" fx:id = "button_origin" onAction = "#goOrigin"/>
        </AnchorPane>
       </AnchorPane>
      </content>
    </Tab>
    <Tab text="Visual">
      <content>
       <AnchorPane id="VisSchemVisual">
        <AnchorPane fx:id="visualContainer" minHeight="0.0" minWidth="0.0" prefHeight="300.0" prefWidth="200.0" />
         <Button layoutX = "10" layoutY = "240" mnemonicParsing = "false" text = "Reset" fx:id = "button_resetPositions" onAction = "#resetPos"/>
         <Button layoutX = "70" layoutY = "240" mnemonicParsing = "false" text = "Deselect All" fx:id = "button_deselectAll" onAction = "#deselect"/>  
       </AnchorPane>
      </content>
    </Tab>
  </tabs>
</TabPane>

